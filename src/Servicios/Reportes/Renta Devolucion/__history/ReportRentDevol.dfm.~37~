  object qData: TUniQuery
    Connection = DM.DBNLND
    SQL.Strings = (
      'SELECT '
      'RD.ID AS CODIGO,'
      'C.NOMBRES AS CLIENTES,'
      'V.DESCRIPCION AS VEHICULOS,'
      'E.NOMBRES AS EMPLEADOS,'
      'RD.FECHA_RENTA,'
      'RD.FECHA_DEVOLUCION,'
      'RD.DIAS,'
      'RD.MONTOXDIA,'
      'CASE WHEN RD.ESTADO = 1 THEN '#39'RENTADO'#39
      'ELSE '#39'DEVUELTO'#39' END AS ESTADO'
      'FROM RENTA_DEVOLUCION RD'
